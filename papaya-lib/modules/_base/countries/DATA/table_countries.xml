<?xml version="1.0" encoding="ISO-8859-1" ?>
<table name="countries" prefix="yes">
<fields>
<field name="country_id" type="string" size="8"  null="no"/>
<field name="country_continent" type="integer" size="4"  null="no"/>
<field name="country_fallbackname" type="string" size="50"  null="no"/>
<field name="country_favorite" type="integer" size="4"  null="no" default="0"/>
<field name="country_priority" type="integer" size="4"  null="no" default="0"/>
<field name="country_comment" type="string" size="65535" null="yes"/>
</fields>
<keys>
<primary-key>
<field>country_id</field></primary-key>
<key name="country_fallbackname">
<field>country_fallbackname</field></key>
</keys>
</table>
